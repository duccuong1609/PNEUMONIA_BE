{
  "version": 2,
  "builds": [
    { "src": "src/index.ts", "use": "@vercel/node" },
    {
      "src": "public/tfjs_model/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "src/index.ts" },
    { "src": "/tfjs_model/(.*)", "dest": "/public/tfjs_model/$1" }
  ],
  "static": {
    "src": "public/**/*",
    "dest": "/"
  }
}
